#!/bin/bash


unzip -qo xterm.js-3.13.0.zip
cp docker-compose.yml  xterm.js-3.13.0/
cp Dockerfile xterm.js-3.13.0/
cp package.json xterm.js-3.13.0/
cp index.html xterm.js-3.13.0/demo
cp client.ts  xterm.js-3.13.0/demo
cp server.js  xterm.js-3.13.0/demo
cp run.js  xterm.js-3.13.0/demo
cp compile_webpack.js  xterm.js-3.13.0/demo
cp go_docker  xterm.js-3.13.0
#cp -R challenges  xterm.js-3.13.0/
cp -R challenges  ctf-tool-xterm/
